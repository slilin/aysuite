<#include "/common/control.ftl"><table class="data list" id="{Helpers.controller}" >	<tr>		<th class="shrink" >			&nbsp;		</th>		<th>			关键字		</th>									<th>			文本		</th>		<th>			编码		</th>	</tr>	<#list myFieldValue as item>		<tr id="${item.valueID}">		<td>							<@ImgQButton id="../../../DeleteFieldValue/${item.valueID}" action="Delete"/>		</td>			<td id="V_F_Key">			${item.v_F_Key?default("")}		</td>					<td id="V_Text">			${item.v_Text?default("")}		</td>		<td id="V_Code">			${item.v_Code?default("")}		</td>		</tr>			</#list></table><@ImgQButton action="CreateFieldValue" auth=false title="新增" icon="Create"/>	<@ImgQButton action="../../Delete/${Helpers.actonId}" auth=false title="删除主字段" icon="Delete"/>		<div id="${Helpers.controller}${Helpers.action}"><form action="" id="${Helpers.controller}-SaveCreateFieldValue" method="post" ><label for="V_F_Key">关键字</label> 	<input type="text" id="V_F_Key" name="V_F_Key" class="text ui-widget-content ui-corner-all" value="" /><label for="V_Text">文本</label> 	<input type="text" id="V_Text" name="V_Text" class="text ui-widget-content ui-corner-all" value="" /> 		<label for="V_Code">编码</label> 	<input type="text" id="V_Code" name="V_Code" class="text ui-widget-content ui-corner-all" value="" /> 		 			<input type="hidden" id="V_FieldID" name="V_FieldID" value="${Helpers.actonId}" />	</form></div>	<script>$(function(){	InitPageParms(this);	var url = $('#${Helpers.controller}thisuri').val();		$("#${Helpers.controller}${Helpers.action}").dialog({		autoOpen: false,		modal: true,		buttons: {			'确定':function(){				$('#${Helpers.controller}-SaveCreateFieldValue').attr('action',url+'/SaveCreateFieldValue')				$('#${Helpers.controller}-SaveCreateFieldValue').ajaxSubmit();				$(this).dialog('close');			},			'取消':function(){				$(this).dialog('close');			}		}	});		$("a[action='${Helpers.controller}.CreateFieldValue']").click(function(){		$("#${Helpers.controller}${Helpers.action}").dialog('open');		return false;	});		})</script>